{
  "name": "booru-processor",
  "version": "1.2.0",
  "description": "Process images with Booru API and save tag data as JSON",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.booru.processor",
    "productName": "Booru Processor",
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "directories": {
      "output": "dist"
    }
  },
  "dependencies": {
    "axios": "^0.27.2",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "electron": "^19.0.0",
    "electron-builder": "^23.6.0"
  }
}
